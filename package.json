{
  "name": "oktech-web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview --host",
    "start": "npm run build && npm run preview",
    "knip": "knip",
    "format": "prettier --write . --log-level warn",
    "typecheck": "npm-run-all -p typecheck:astro typecheck:tsc",
    "typecheck:tsc": "tsc --noEmit",
    "typecheck:astro": "astro check",
    "checks": "npm-run-all format typecheck knip",
    "import": "tsx ./scripts/import-data",
    "og-cache": "tsx scripts/manage-og-cache.ts",
    "extract-themes": "tsx scripts/extract-theme-colors.ts",
    "analyze-bundle": "ANALYZE_BUNDLE=true astro build && echo '\nðŸ“Š Bundle Analysis Complete! ðŸ“Š\n\nðŸ”— View stats at: http://localhost:4321/stats.html\n' && npm run preview",
    "test": "npm run test:dev",
    "test:dev": "playwright test",
    "test:screenshots": "rm -rf test/screenshots/output && TEST_TYPE=screenshots playwright test",
    "test:build": "npm run build && TEST_BUILD=true playwright test",
    "test:dist": "TEST_BUILD=true playwright test"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@fontsource/lexend": "^5.2.11",
    "@playwright/test": "^1.57.0",
    "@react-spring/web": "^10.0.3",
    "@resvg/resvg-js": "^2.6.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.6",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "daisyui": "^5.5.14",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "knip": "^5.76.3",
    "mdast-util-to-string": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "osm-static-maps": "^4.0.2",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "puppeteer": "^24.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-photo-album": "^3.3.0",
    "reading-time": "^1.5.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "satori": "^0.18.3",
    "satori-html": "^0.3.2",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-to-css": "^0.0.12",
    "typescript": "^5.9.3",
    "unist-util-visit": "^5.0.0",
    "vite": "^6.4.1",
    "vite-plugin-svgr": "^4.5.0",
    "yaml": "^2.8.2",
    "yet-another-react-lightbox": "^3.27.0"
  }
